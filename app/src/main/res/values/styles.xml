<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColor">#13130d</item>


    </style>

    <style name="Button" parent="Widget.AppCompat.Button">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#01010b</item>
        <item name="android:background">#8989da</item>
        <item name="android:layout_marginBottom">2dip</item>
        <item name="android:textStyle">italic</item>
        <item name="android:layout_margin">4dip</item>


    </style>
</resources>
